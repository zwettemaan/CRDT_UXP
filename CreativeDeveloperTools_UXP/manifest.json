{
  "id": "creative-developer-tools-uxp-sample",
  "name": "creative-developer-tools-uxp-sample",
  "version": "0.0.1",
  "main": "index.html",
  "host": [ 
    {
      "app": "ID",
      "minVersion": "19.0"
    },
    {
      "app": "PS",
      "minVersion": "23.0"
    }
  ],
  "manifestVersion": 5,
  "requiredPermissions": {
    "network": {
      "domains": [
        "localhost.tgrg.net"
      ]
    }
  },
  "entrypoints": [
    {
      "type": "panel",
      "id": "showPanel",
      "minimumSize": {
        "width": 700,
        "height": 300
      },
      "maximumSize": {
        "width": 2000,
        "height": 2000
      },
      "preferredDockedSize": {
        "width": 700,
        "height": 300
      },
      "preferredFloatingSize": {
        "width": 700,
        "height": 300
      },
      "label": {
        "default": "Creative Developer Tools Sample"
      },
      "icons": [
        {
          "width": 23,
          "height": 23,
          "path": "icons/dark.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "darkest",
            "dark",
            "medium"
          ]
        },
        {
          "width": 23,
          "height": 23,
          "path": "icons/light.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "lightest",
            "light"
          ]
        }
      ]
    }
  ],
  "icons": [
    {
      "width": 48,
      "height": 48,
      "path": "icons/plugin.png",
      "scale": [
        1,
        2
      ],
      "theme": [
        "darkest",
        "dark",
        "medium",
        "lightest",
        "light",
        "all"
      ],
      "species": [
        "pluginList"
      ]
    }
  ]
}